<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="actor1" class="com.stackroute.spring.domain.Actor">
        <property name="actorName" value="surya"></property>
        <property name="actorGender" value="male"></property>
        <property name="actorAge" value="30"></property>
    </bean>
    <bean id="movie" autowire="byName" class="com.stackroute.spring.domain.Movie">
        <property name="actor" ref="actor1"></property>
    </bean>
</beans>