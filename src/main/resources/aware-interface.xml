<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="actor" class="com.stackroute.spring.domain.Actor">
        <property name="actorName" value="Ajith"></property>
        <property name="actorGender" value="male"></property>
        <property name="actorAge" value="45"></property>
    </bean>
    <bean id="movie" autowire="byType" class="com.stackroute.spring.domain.Movie">
        <property name="actor" ref="actor"></property>
    </bean>
    <bean id="lifecycledemo" class="com.stackroute.spring.domain.LifeCycleDemo" init-method="customInit" destroy-method="customDestroy">

    </bean>

    <bean id="postprocessor" class="com.stackroute.spring.domain.PostProcessorDemo">

    </bean>
</beans>